{%extends 'templates/app.twig'%}
{% block head %}
<link rel="stylesheet" href="{{baseUrl}}/resources/css/templates/footable.bootstrap.min.css">
<link rel="stylesheet" href="{{baseUrl}}/resources/node_modules/shepherd.js/dist/css/shepherd-theme-arrows.css"/>
<link rel="stylesheet" href= "{{baseUrl}}/resources/css/home.css">
{% endblock %}
{% block content %}
<script type="text/javascript" src="{{baseUrl}}/resources/js/templates/konva.js"></script>
  <div class="elements-tray">
    <div class="btn-group-vertical" role="group">
       <button type="button" id="add" class="btn btn-dark" style="border-top-left-radius:0">{%include 'templates/svg/resistor.svg'%}</button>
       <button type="button" id="in" class="btn btn-dark">
         <svg height="30" width="30"> <circle cx="15" cy="15" r="8"  fill="#fff" /> </svg>
       </button>
       <button type="button" id="calc" class="btn btn-dark" style="border-bottom-left-radius:0"><i class="fas fa-cogs"></i></button>
     </div>
  </div>
  <div id="container" class="canvas">
     {%include 'templates/svg/grid.svg'%}
    </div>
  <div class="result"></div>
  <footer>
    <p>Copyright (c) 2019 Electro All Rights Reserved</p>
  </footer>
 <div class="edit">
   <div class="input-group">
    <input type="text" id="edit-value" class="form-control pr-1" aria-describedby="button-addon4" style="width:50px;">
    <div class="input-group-append" id="button-addon4">
      <button type="button" class="btn btn-primary" id="edit-check"><i class="fas fa-check"></i></button>
      <button type="button" class="btn btn-primary" id="edit-rl"><i class="fas fa-undo"></i></button>
      <button type="button" class="btn btn-primary" id="edit-rr" ><i class="fas fa-redo"></i></button>
      <button type="button" class="btn btn-primary" id="edit-delete"><i class="fas fa-trash"></i></button>
    </div>
  </div>
 </div>
  <script type="text/javascript" src="{{baseUrl}}/resources/js/home.js"></script>
{% endblock %}
